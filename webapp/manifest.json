{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "pd.pm.lite",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"ach": "CA-UI5-DOC",
		"dataSources": {}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		},
		"supportedThemes": [
			"sap_bluecrystal"
		],
		"fullWidth": true
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "pd.pm.lite.view.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pd.pm.lite.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "pd.pm.lite.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "appHome",
					"target": "home"
				},
				{
					"pattern": "personas",
					"name": "personas",
					"target": "personas"
				},
				{
					"pattern": "ChangeOrder/{order}",
					"name": "changeOrder",
					"target": "changeOrder"
				},
				{
					"pattern": "NewOrder",
					"name": "newOrder",
					"target": "newOrder"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				},
				"personas": {
					"viewName": "personas",
					"viewLevel": 2
				},
				"changeOrder": {
					"viewName": "ChangeOrder",
					"viewId": "changeView",
					"viewLevel": 2
				},
				"newOrder": {
					"viewName": "NewOrder",
					"viewId": "newView",
					"viewLevel": 2
				}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zpm_lite_v1/webapp",
		"_version": "1.1.0"
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}