<core:FragmentDefinition xmlns="sap.m" xmlns:smartTable="sap.ui.comp.smarttable" xmlns:core="sap.ui.core">
	<Dialog id="idPartStatusDialog" title="{i18n>PartStatus}">
		<Table items="{PartStatus}">
			<columns>
				<Column width="3rem">
					<Text text="Status"/>
				</Column>
				<Column width="10rem">
					<Text text="Material"/>
				</Column>
				<Column width="2rem">
					<Text text="Req"/>
				</Column>
				<Column width="2rem">
					<Text text="Wtdr"/>
				</Column>
				<Column width="3rem">
					<Text text="OnHn"/>
				</Column>
				<Column width="5rem">
					<Text text="Bin"/>
				</Column>
				<Column width="2rem">
					<Text text="G"/>
				</Column>
				<Column width="6rem">
					<Text text="Purch Req"/>
				</Column>
				<Column width="6rem">
					<Text text="Purch Ord"/>
				</Column>
				<Column width="2rem">
					<Text text="Qty"/>
				</Column>
				<Column width="2rem">
					<Text text="Cnt"/>
				</Column>
				<Column width="2rem">
					<Text text="FF"/>
				</Column>
				<Column width="4rem">
					<Text text="FF Date"/>
				</Column>
				<Column width="2rem">
					<Text text="Rig"/>
				</Column>
				<Column width="4rem">
					<Text text="Rig Date"/>
				</Column>
				<Column width="5rem">
					<Text text="MoT"/>
				</Column>
				<Column width="5rem">
					<Text text="ETA Dest"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<core:Icon color="{path:'Status', formatter: '.formatter.statusIconColor'}" src="{path:'Status', formatter: '.formatter.statusIcon'}"/>
						<ObjectIdentifier title="{Name}" text="{MaterialNumber}"/>
						<Text text="{RequiredQuantity}"/>
						<Text text="{WithdrawnQuantity}"/>
						<Text text="{OnHandQuantity}"/>
						<Text text="{BinNumber}"/>
						<Text text="{FreightGroup}"/>
						<Text text="{PRNumber}"/>
						<Text text="{PONumber}"/>
						<Text text="{POQuantity}"/>
						<Text text="{OpenPOCount}"/>
						<Text text="{FFReceivedQuantity}"/>
						<Text text="{FFReceivedDate}"/>
						<Text text="{RRQuantity}"/>
						<Text text="{RRDate}"/>
						<Text text="{ModeOfTransport}"/>
						<Text text="{ETAPortOfEntry}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		<beginButton>
			<Button text="Close" press=".closeDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>